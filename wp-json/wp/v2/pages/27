ormatCurrency(loanAmount);\n    }\n\n    loanSlider.addEventListener(\"input\", updateAmountDisplay);\n    updateAmountDisplay();\n  } else {\n    console.error(\"Slider input or amount display not found.\");\n  }\n});\n<\/script>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div data-particle_enable=\"false\" data-particle-mobile-disabled=\"false\" class=\"elementor-element elementor-element-1eb69ee e-con-full e-flex e-con e-child\" data-id=\"1eb69ee\" data-element_type=\"container\">\n\t\t\t\t<div class=\"elementor-element elementor-element-c36c7dc elementor-widget__width-initial elementor-widget elementor-widget-ucaddon_dropdown_field\" data-id=\"c36c7dc\" data-element_type=\"widget\" data-widget_type=\"ucaddon_dropdown_field.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t\n<!-- start Dropdown Field -->\n\t\t<link id='font-awesome-css' href='https:\/\/stjohncommunitycare.com\/wp-content\/plugins\/unlimited-elements-for-elementor-premium\/assets_libraries\/font-awesome6\/fontawesome-all.min.css' type='text\/css' rel='stylesheet' >\n\t\t<link id='font-awesome-4-shim-css' href='https:\/\/stjohncommunitycare.com\/wp-content\/plugins\/unlimited-elements-for-elementor-premium\/assets_libraries\/font-awesome6\/fontawesome-v4-shims.min.css' type='text\/css' rel='stylesheet' >\n\n<style>\/* widget: Dropdown Field *\/\n\n#uc_dropdown_field_elementor_c36c7dc.ucform-has-conditions {\n\tdisplay: none;\n}\n\n\n#uc_dropdown_field_elementor_c36c7dc\n{  \n  display: flex;\n  flex-wrap: wrap;  max-width:100%;\n}\n#uc_dropdown_field_elementor_c36c7dc .ue-dropdown-field-wrapper{\n  width:100%;\n}\n\n\n#uc_dropdown_field_elementor_c36c7dc label{\n  display:block;\n  }\n\n#uc_dropdown_field_elementor_c36c7dc .ue-dropdown-field-wrapper{\n    position: relative;\n\n  }\n\n#uc_dropdown_field_elementor_c36c7dc .uc-select-field_select-indicator{\n\tposition: absolute;\n  \ttop: 50%;\n  \ttransform: translate(0, -50%);\n  \tz-index: 1;\n  \theight: 100%;\n    display: f